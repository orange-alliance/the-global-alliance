{
  "name": "the-global-alliance",
  "version": "0.0.1",
  "description": "Official Live Results For FIRST Global",
  "scripts": {
    "lint": "tslint --test",
    "watch-client": "webpack --config ./config/webpack.client.config.js --watch",
    "watch-server": "webpack --config ./config/webpack.server.config.js --watch",
    "build-client": "webpack --config ./config/webpack.client.config.js",
    "build-server": "webpack --config ./config/webpack.server.config.js",
    "build": "npm run build-client && npm run build-server",
    "build-client-dev": "webpack --config ./config/webpack.client.config.js --env.NODE_ENV=development",
    "build-server-dev": "webpack --config ./config/webpack.server.config.js --env.NODE_ENV=development",
    "build-dev": "npm run build-client-dev && npm run build-server-dev",
    "start": "nodemon build/server/index.js --config nodemon.json"
  },
  "author": "Kyle Flynn",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.3.1",
    "@material-ui/icons": "^4.2.1",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "axios": "^0.19.0",
    "axios-hooks": "^1.2.1",
    "express": "^4.17.1",
    "path": "^0.12.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "webpack-dev-middleware": "^3.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/react-router-dom": "^4.3.4",
    "@types/webpack": "^4.32.1",
    "@types/webpack-dev-middleware": "^2.0.3",
    "babel-loader": "^8.0.6",
    "nodemon": "^1.19.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
